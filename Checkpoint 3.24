import java.util.Scanner;

public class Checkpoint 
{
     public static void main(String[] args)
     {
     
     byte userNum;
     
     String userString;
     
     //Create scanner object
     Scanner keyboard = new Scanner(System.in);
     
     //Get user input
     System.out.println("Enter one of the numbers " +
                        "1, 2, or 3: ");
     
     userNum = keyboard.nextByte();
     
     switch(userNum) {
     case 1: userString = " One";
          break;
     
     case 2: userString = "Two";
          break;
     
     case 3: userString = "Three";
          break;

     default : userString = "Invalid entry, only enter 1, 2, or 3.";
          break;
          
          }
     }
}
